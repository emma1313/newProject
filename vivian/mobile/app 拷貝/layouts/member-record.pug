        .tab-pane.fade.show.py-4#pills-tabContent
            //投注紀錄
            .tab-pane.fade.show#pills-withdraw        
                //content
                #accordion2.member-accordion
                    //投注_投注紀錄
                    .member-card
                        .member-card_header#headingTwo-withdraw
                            h5.mb-0
                                button.btn.link-lightBlue.collapsed.d-flex.justify-content-between.align-items-center.px-0(data-toggle='collapse', data-target='#collapseTwo-withdraw', aria-expanded='true')
                                    | 投注紀錄
                                    i.fas.fa-angle-up.fa-lg
                        #collapseTwo-withdraw.collapse(data-parent='#accordion')
                            .member-card_body
                                form.needs-validation
                                    //投注時間
                                    .form-group.row
                                        label.col-12.col-form-label 投注時間
                                        .col-12
                                            select.form-control
                                                option(value="1") 今天
                                                option(value="2") 一週
                                                option(value="2") 一個月
                                    //投注遊戲
                                    .form-group.row
                                        label.col-12.col-form-label 投注遊戲
                                        .col-12
                                            select.form-control
                                                option(value="1") 全部
                                                option(value="2") 歐博
                                                option(value="2") 沙龍
                                    //btn 
                                    .row
                                        .col-12.mt-4.mb-5
                                            button.btn.btn-primary.px-5(type='submit', value='Submit') 送出
                                    //table
                                    .form-group.row
                                        .col-12
                                            h6.text-secondary.mb-4
                                                i.far.fa-calendar-alt.fa-fw
                                                | 明細時間：一週 
                                            button.btn.btn-outline-primary.px-5.mb-2(type='submit', value='Submit') 
                                                i.fas.fa-redo.mr-2
                                                | 重新整理
                                    .form-group.row
                                        .col-12
                                            .table-responsive
