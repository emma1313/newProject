        //member-pill_content
        .tab-pane.fade.show.py-4#pills-tabContent
            //存款 
            .tab-pane.fade.show#pills-deposit        
                //content
                #accordion2.member-accordion
                    //存款_存款管理
                    .member-card
                        .member-card_header#headingOne-deposit
                            h5.mb-0
                                button.btn.link-lightBlue.collapsed.d-flex.justify-content-between.align-items-center.px-0(data-toggle='collapse', data-target='#collapseOne-deposit', aria-expanded='true')
                                    |存款管理
                                    i.fas.fa-angle-up.fa-lg
                        #collapseOne-deposit.collapse(data-parent='#accordion')
                            .member-card_body
                                form.needs-validation(novalidate)
                                    //存款方式
                                    .form-group.row
                                        label.col-12.col-form-label(required) 
                                            |存款方式
                                            // 存款方式 Modal
                                            i.far.fa-question-circle.fa-lg.link-lightBlue.pl-3( data-toggle='modal', data-target='#questionModalCenter')
                                        .col-12
                                            select.form-control(required)
                                                option(value="1") 超商代码
                                                option(value="2") 虛擬帐号ATM
                                                option(value="3") 银行帐户
                                    //存款額度
                                    .form-group.row
                                        label.col-12.col-form-label 存款額度
                                        .col-12
                                            input.form-control(type='text', placeholder='请输入存款額度' required)
                                            .font-14.mt-1 需扣除額外手續費15元

                                            //alert
                                            .alert.alert-info.mt-4
                                                strong 存款申請中
                                                |  詳細紀錄請至存款紀錄查詢，並請耐心等候，本公司客服人員將盡速通知您申請結果。

                                    //btn 
                                    .row
                                        .col-12.mt-4.mb-5
                                            button.btn.btn-primary.px-5(type='submit', value='Submit') 送出
                                            button.btn.btn-outline-secondary.px-5(type='reset', value='Reset') 重填
                    //存款_存款紀錄
                    .member-card
                        .member-card_header#headingTwo-deposit
                            h5.mb-0
                                button.btn.link-lightBlue.collapsed.d-flex.justify-content-between.align-items-center.px-0(data-toggle='collapse', data-target='#collapseTwo-deposit', aria-expanded='true')
                                    | 存款紀錄
                                    i.fas.fa-angle-up.fa-lg
                        #collapseTwo-deposit.collapse(data-parent='#accordion')
                            .member-card_body
                                form.needs-validation
                                    //存款時間
                                    .form-group.row
                                        label.col-12.col-form-label 存款時間
                                        .col-12
                                            select.form-control
                                                option(value="1") 今天
                                                option(value="2") 一週
                                                option(value="2") 一個月
                                    //存款狀態
                                    .form-group.row
                                        label.col-12.col-form-label 存款狀態
                                        .col-12
                                            select.form-control
                                                option(value="1") 全部
                                                option(value="2") 成功
                                                option(value="3") 存款審核中
                                                option(value="3") 等待储值
                                                option(value="3") 處理中
