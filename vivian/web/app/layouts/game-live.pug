// Modal transfer
.modal.fade#gameLiveModalCenter(tabindex='-1', role='dialog', aria-labelledby='gameLiveModalCenterTitle', aria-hidden='true')
    .modal-dialog.modal-dialog-centered.modal-lg(role='document')
        .modal-content
            .modal-header
                h5#gameLiveModalLongTitle.modal-title 
                    i.fas.fa-wallet.mr-2
                    |點數管理
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    i.fas.fa-times(aria-hidden='true')
            .modal-body.m-3
                form
                    //點數
                    .form-group.row
                        .col-2
                            i.fas.fa-wallet.pr-2 
                            |錢包點數
                        .col-4
                            span.pl-3 12345678.99
                            i.fas.fa-redo.c-blueDark.pl-3.cursor-p
                        .col-2 
                            i.fas.fa-dollar-sign.pr-2 
                            |沙龍額度
                        .col-4
                            span.pl-3 12345678.99
                            i.fas.fa-redo.c-blueDark.pl-3.cursor-p
                    //轉入遊戲
                    .form-group.row
                        .col-2.d-flex.align-items-center
                            i.fas.fa-gamepad.pr-2 
                            | 轉入遊戲
                        .col-10.d-flex.align-items-center.pl-4
                            input.form-control.border.border-primary.w-45.ml-1.py-2(type='text', placeholder='请输入點數')
                    //點數
                    .form-group.row
                        .col-2.d-flex.align-items-center
                            i.fas.fa-exchange-alt.pr-2
                            | 快速轉點
                        .col-10.pl-4
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 100
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 300
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 500
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 1000
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 3000
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 5000
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 10000
                            button.btn.btn-secondary.w-30.mx-1.my-2.py-2(type='button') 30000
                            button.btn.btn-dark.w-30.mx-1.my-2.py-2(type='button') 全部點數
                    //進入遊戲
                    .form-group.row
                        .col-10.offset-2.pl-4
                            button.btn.btn-primary.w-45.mx-1.py-2(type='button') 
                                i.fas.fa-exchange-alt.pr-2
                                |轉點並進入游戏
                            button.btn.btn-outline-primary.w-45.mx-3.py-2(type='button') 
                                i.fas.fa-long-arrow-alt-right.pr-2
                                |直接進入游戏

// Modal auto transfer
.modal.fade#gameLiveAutoModalCenter(tabindex='-1', role='dialog', aria-labelledby='gameLiveAutoModalCenterTitle', aria-hidden='true')
    .modal-dialog.modal-dialog-centered(role='document')
        .modal-content
            .modal-header
                h5#gameLiveAutoModalLongTitle.modal-title 
                    i.fas.fa-wallet.mr-2
                    |免轉錢包
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    i.fas.fa-times(aria-hidden='true')
            .modal-body.m-3
                form
                    //點數
                    .form-group.row
                        .col-12.h5.d-flex.justify-content-center.align-items-center.mt-5
                            i.fas.fa-wallet.pr-2
                            span.pr-2 錢包點數
                            span.pr-3 12345678.99
                            i.fas.fa-redo.c-blueDark.cursor-p
                    //進入遊戲
                    .form-group.row
                        .col-12
                            button.btn.btn-primary.w-100.mt-5.py-2#enterGameBtn(type='button') 進入游戏
                            .progress.mt-5.d-none#progressBar(style='height: 42px')
                                .progress-bar.progress-bar-striped.live-progress(role='progressbar') 55%

// 登入 Modal
.modal.fade#ModalFull(tabindex='-1', role='dialog', aria-labelledby='exampleModalCenterTitle', aria-hidden='true')
    .modal-dialog.modal-dialog-full(role='document')
        .modal-content.border-0
            .modal-body.p-0
                button.close.p-4(type='button', data-dismiss='modal', aria-label='Close')
                    i.fas.fa-times.m-dark(aria-hidden='true')
                .row
                    .col-5
                        .modal-checkIn_side
                            .checkIn-side_title.d-flex.justify-content-center
                                h5.fadeInUp.animated 百款遊戲讓你愛不釋手
                            .checkIn-side_slider
                                ul
                                    li
                                        img(src="./images/checkIn-slider1.png", alt="images slider1")
                                    li
                                        img(src="./images/checkIn-slider2.png", alt="images slider2")
                                    li
                                        img(src="./images/checkIn-slider3.png", alt="images slider3")
                    .col-7
                        .modal-checkIn_main
                            .box-shadow
                            .d-flex.align-items-center.ml-5
                                .checkIn-main_days.fadeInDown.animated.slow 本月簽到天數 
                                    span 5 
                                .d-flex.justify-content-start.flex-wrap.ml-5.w-100 
                                        h2.checkIn-main_title.fadeInUp.animated.slow
